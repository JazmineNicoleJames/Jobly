{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "job": {
            "type": "object",
            "default": {},
            "title": "The job Schema",
            "required": [
                "title",
                "salary",
                "equity",
                "company_handle"
            ],
            "properties": {
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "fakeTitle"
                    ]
                },
                "salary": {
                    "type": "integer",
                    "default": 0,
                    "title": "The salary Schema",
                    "examples": [
                        1000
                    ]
                },
                "equity": {
                    "type": "integer",
                    "default": 0,
                    "title": "The equity Schema",
                    "examples": [
                        0
                    ]
                },
                "company_handle": {
                    "type": "string",
                    "default": "",
                    "title": "The company_handle Schema",
                    "examples": [
                        "c2"
                    ]
                }
            },
            "examples": [{
                "title": "fakeTitle",
                "salary": 1000,
                "equity": 0,
                "company_handle": "c2"
            }]
        }
    },
    "examples": [{
        "job": {
            "title": "fakeTitle",
            "salary": 1000,
            "equity": 0,
            "company_handle": "c2"
        }
    }]
}